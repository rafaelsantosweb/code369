{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.9",
        "drupal/admin_toolbar": "^3.3",
        "drupal/backup_migrate": "^5.0",
        "drupal/bamboo_twig": "^6.0",
        "drupal/block_field": "^1.0@RC",
        "drupal/blocktabs": "^2.0@alpha",
        "drupal/colorbutton": "^1.5",
        "drupal/components": "^3.0@beta",
        "drupal/conditional_fields": "^4.0@alpha",
        "drupal/config_ignore": "^3.0@beta",
        "drupal/config_split": "^1.9",
        "drupal/context": "^5.0@RC",
        "drupal/core-composer-scaffold": "^10",
        "drupal/core-project-message": "^10",
        "drupal/core-recommended": "^10",
        "drupal/devel": "^5.1",
        "drupal/diff": "^1.1",
        "drupal/dotenv": "^1.1",
        "drupal/draggableviews": "^2.1",
        "drupal/easy_breadcrumb": "^2.0",
        "drupal/ebt_tabs": "^1.4",
        "drupal/editor_advanced_image": "^2.1",
        "drupal/editor_advanced_link": "^2.2",
        "drupal/environment_indicator": "^4.0",
        "drupal/fullcalendar_view": "^5.1",
        "drupal/google_tag": "^2.0",
        "drupal/http_client_manager": "*",
        "drupal/linkit": "^6.1",
        "drupal/mask": "^2.0@alpha",
        "drupal/menu_link_attributes": "^1.3",
        "drupal/module_filter": "^4.0",
        "drupal/node_revision_delete": "^2.0@alpha",
        "drupal/page_attributes": "^2.0",
        "drupal/panelbutton": "^1.5",
        "drupal/paragraphs": "^1.15",
        "drupal/paragraphs_collapsible": "^1.4",
        "drupal/paragraphs_limits": "^1.0@alpha",
        "drupal/pathauto": "^1.11",
        "drupal/permissions_by_term": "^3.1",
        "drupal/permissions_filter": "^1.3",
        "drupal/quick_node_clone": "^1.16",
        "drupal/radix": "^5.0",
        "drupal/redis": "^1.6",
        "drupal/remove_http_headers": "^2.0",
        "drupal/robotstxt": "^1.5",
        "drupal/schema_metatag": "^3.0",
        "drupal/search_api": "^1.29",
        "drupal/seckit": "^2.0",
        "drupal/smtp": "^1.2",
        "drupal/stage_file_proxy": "^2.0",
        "drupal/tagclouds": "^2.0",
        "drupal/tagify": "^1.0",
        "drupal/tiny_slider": "^1.0@beta",
        "drupal/token": "^1.11",
        "drupal/twig_tweak": "^3.2",
        "drupal/user_redirect": "^2.0",
        "drupal/views_field_view": "^1.0@beta",
        "drupal/views_infinite_scroll": "^2.0",
        "drupal/viewsreference": "^2.0@beta",
        "drupal/webform": "^6.2@beta",
        "drupal/weight": "^3.4",
        "drush/drush": "*",
        "kint-php/kint": "^5.0",
        "predis/predis": "^2.2"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "process-timeout": 0,
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "sort-packages": true
    },
     "autoload": {
        "files": ["load.environment.php"]
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ]
        }
    },
    "require-dev": {
        "chi-teck/drupal-code-generator": "^2.6",
        "drupal/coder": "^8.3",
        "squizlabs/php_codesniffer": "^3.7"
    }
}
